{
  "name": "dm-tree-sitter",
  "version": "0.1.0",
  "description": "DreamMaker grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "dreammaker",
    "byond"
  ],
  "author": "mcbalaam",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "node-gyp-build": "^4.5.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8",
    "prebuildify": "^6.0.0"
  },
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.dm",
      "file-types": ["dm", "dme"],
      "highlights": "queries/highlights.scm"
    }
  ]
}
