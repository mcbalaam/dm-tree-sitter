{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dm_tree_sitter",
      "camelcase": "DmTreeSitter",
      "title": "dm-tree-sitter",
      "scope": "source.dm_tree_sitter",
      "file-types": [
        "dm_tree_sitter"
      ],
      "injection-regex": "^dm_tree_sitter$",
      "class-name": "TreeSitterDmTreeSitter"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "DmTreeSitter grammar for tree-sitter",
    "authors": [
      {
        "name": "mcbalaam"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-dm_tree_sitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}